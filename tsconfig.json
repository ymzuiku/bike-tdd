{
  "typedocOptions": {
    "mode": "modules",
    "out": "docs"
  },
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext", "webworker"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmitHelpers": false,
    "importHelpers": true,
    "noResolve": false,
    "noEmit": true,
    "inlineSourceMap": false,
    "declaration": false,
    "outDir": "./umd",
    "jsx": "preserve"
  },
  "exclude": [".vscode", ".git", "node_modules", "umd", "dist"],
  "include": ["lib", "tdd.js", "test/index.test.ts"]
}
